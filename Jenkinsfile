node {
   
    stage('Maven version') {
	  sh 'mvn --version'
      
    }
	   stage('Java version') {
	   sh 'java -version'
      
    }     
	stage('Java version') {
	  sh 'java --version'
    } 
     stage('Jenkins version') {
	  sh 'jenkins --version'
    }
    stage('Maven version') {
	  sh 'mvn --version'
    }
     stage('Vijay') {
	  echo " Vijay World "
    } 	
}
